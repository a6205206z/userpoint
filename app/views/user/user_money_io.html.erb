
<% content_for :application do %>
	<h1>User Money IO</h1>
	<% if @money_ios%>
		<table border=1>
			 <tr>
				  <td>状态</td>
				  <td>金额</td>
				  <td>备注</td>
				  <td>操作时间</td>
			  </tr>
			<% @money_ios.each do |m| %>
			  <tr>
				  <td>
				  	<% if m.status == 1 %>
				  	<p style="color:green">成功</p>
				  	<% elsif m_status == 0 %>
				  	<p style="color:red">失败</p>
				  	<% end %>
				  </td>
				  <td><%= m.money %> 元</td>
				  <td><%= m.remarks %></td>
				  <td><%= m.operate_time %></td>
			  </tr>
			<% end %>
		</table>
	<% end %>
<% end %>
